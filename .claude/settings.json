  "disabledMcpjsonServers": [
    "claude"
  ],
  "enabledMcpjsonServers": [
    "documentation",
    "logic",
    "memory",
    "time"
  ],
  "env": {
    "DISABLE_AUTOUPDATER": "1"
  },
  "permissions": {
